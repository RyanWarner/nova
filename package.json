    "connected-react-router": "6.8.0",
    "dotenv": "8.2.0",
    "focus-visible": "5.1.0",
    "history": "5.0.0",
    "hoist-non-react-statics": "3.3.2",
    "informed": "3.9.0",
    "lodash": "4.17.15",
    "prism-react-renderer": "1.1.1",
    "react": "16.13.1",
    "react-aria-modal": "4.0.0",
    "react-dom": "16.13.1",
    "react-feather": "2.0.8",
    "react-ga": "3.1.2",
    "react-helmet": "6.1.0",
    "react-redux": "7.2.0",
    "react-router": "5.2.0",
