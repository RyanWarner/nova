{
  "name": "@ryan-warner/nova",
  "version": "1.0.0",
  "author": "Ryan Warner (https://github.com/RyanWarner)",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "deploy:production": "npm run build:production && cross-env DEPLOY_ENV=production node scripts/deploy.js",
    "deploy:staging": "npm run build:staging && cross-env DEPLOY_ENV=staging node scripts/deploy.js",
    "deploy:develop": "npm run build:develop && cross-env DEPLOY_ENV=develop node scripts/deploy.js",
    "build:production": "cross-env NODE_ENV=production DEPLOY_ENV=production node scripts/build.js",
    "build:staging": "cross-env NODE_ENV=production DEPLOY_ENV=staging node scripts/build.js",
    "build:develop": "cross-env NODE_ENV=production DEPLOY_ENV=develop node scripts/build.js",
    "build:with-stats": "cross-env NODE_ENV=production webpack --config config/webpack.config.js/client.prod.js --json > build/client/static/bundle-stats.json",
    "plop": "plop",
    "start": "cross-env NODE_ENV=development node scripts/start.js",
    "start:with-analyzer": "cross-env ANALYZER=true npm start",
    "lint:fix": "eslint src/** --fix",
    "storybook": "start-storybook -s .storybook/images",
    "build-storybook": "build-storybook -s .storybook/images",
    "deploy:storybook": "storybook-to-ghpages",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:ci": "npm-run-all --race start cy:run"
  },
  "moduleRoots": [
    "src",
    "node_modules"
  ],
  "dependencies": {
    "@babel/runtime": "7.10.4",
    "@sentry/browser": "5.19.0",
